<UserControl x:Class="QueryWindow.Navigation.Open"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QueryWindow.Navigation"
             xmlns:infra="clr-namespace:Friend.Infra;assembly=Friend.Infra"
             xmlns:views="clr-namespace:QueryWindow.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button Content="Query Window" Command="{x:Static infra:ApllicationCommands.NavigationCommand}" 
                CommandParameter="{x:Type views:Main}"/>
    </Grid>
</UserControl>
